<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net472;netstandard2.0;netstandard2.1;netcoreapp3.0</TargetFrameworks>

    <Description>
      Provides functionality for registering Moq mocks with AutoFixture fixtures.
    </Description>
    <PackageTags>testing;autofixture;mocking;moq</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AutoFixture" Version="[4.0.0, 5.0.0)" />
    <PackageReference Include="Moq" Version="[4.11.0, 5.0.0)" />
  </ItemGroup>

</Project>
